
class http{
	getbinary(url){
		return signal{
			var req = new XMLHttpRequest()
			req.open("GET", url, true)
			req.responseType = 'arraybuffer'
			req.onreadystatechange = function(){
				if(req.readyState == 4){
					if(req.status != 200) return signal.throw(req.status)
					var value = req.response
					return signal.end(value)
				}
			}
			req.send()
		}
	}
}